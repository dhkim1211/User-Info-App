doctype html
html
	head
		link(href='/css/main.css', rel='stylesheet', type='text/css')
	body
		div.menu
			ul.nav
				li.title
					strong userInfoApp
				li 
					a(href="/") allUsers
				li 
					a(href="search") searchUsers
				li 
					a(href="newuser") newUser

		h2 Matches:
			ul.users
				each name in nameMatches
					li #{name.firstname} #{name.lastname} - #{name.email}
